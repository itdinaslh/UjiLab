@{
    ViewBag.Title = "Master Data Metode Sampling";
}

@section plugincss {
    <link rel="stylesheet" href="~/assets/css/datatables.css" type="text/css" />
    <link rel="stylesheet" href="~/lib/sweetalert/sweetalert2.min.css" />
}

<!--Modal Window-->
<div id='myModal' class='modal fade in' role="dialog" data-bs-keyboard="false" data-bs-backdrop="static">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div id='myModalContent'></div>
        </div>
    </div>
</div>

<!-- Page Header -->
<div class="container-fluid">
    <div class="page-header">
        <div class="row">
            <div class="col-sm-6">
                <h3>Metode Sampling</h3>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">Master Data</li>
                    <li class="breadcrumb-item">Metode Sampling</li>
                </ol>
            </div>
            <div class="col-sm-6">
                <div class="bookmark">
                    <ul>
                        <li>
                            <a href="javascript:void(0)" class="showMe" data-href="/master/metode-sampling/create" data-container="body" data-bs-toggle="popover" data-placement="top" title="" data-original-title="Add">
                                <i data-feather="plus"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Content -->
<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <div class="card">
                <div class="card-body">
                    <div class="">
                        <table id="tblData" class="display datatables">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Nama Metode</th>
                                    <th>Kode</th>
                                    <th>Deskripsi</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section pluginjs {
    <script src="~/assets/js/datatable/datatables/jquery.dataTables.min.js"></script>
    <script src="~/lib/sweetalert/sweetalert2.all.min.js"></script>
}

@section scripts {
    <script src="~/js/modalForm.js"></script>
    <script src="~/pages/master/metode.js"></script>
}