@model BakuMutu

<form action="/master/baku-mutu/store" method="post">
    <div class="modal-header">
        @if (Model.BakuMutuID != 0)
        {
            <h5 class="model-title">Edit Master Baku Mutu</h5>
        }
        else
        {
            <h5 class="modal-title">Tambah Master Baku Mutu</h5>
        }
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
    </div>
    <div class="modal-body">
        <input type="hidden" asp-for="BakuMutuID">
        <div class="form-group">
            <label>Nama Baku Mutu</label>
            <input type="text" asp-for="NamaBakuMutu" class="form-control" autofocus autocomplete="off" />
            <span asp-validation-for="NamaBakuMutu" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label>Output Hasil Uji</label>
            <select id="output" class="form-control">

            </select>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-success">Save</button>
    </div>
</form>
